}

.image-list-group{
  padding-top: 20rpx;
  display:flex;
  justify-content: flex-start;
  flex-wrap: wrap;
}

.image-list-group .image-group{
   margin-right: 3rpx;
  margin-bottom: 3rpx;
  position: relative;

}

.image-list-group .image-group image{
  width: 100%;
  height: 100%;
}

.image-list-group .image-group .close-btn{
  width:60rpx;
  height: 60rpx;
  position: absolute;
  right: -20rpx;
  top: -20rpx;
  border-radius: 100%;
  background-size: 50rpx;
  z-index: 1;
  background-repeat: no-repeat;
  background-position: center;
  background-image: url("data:image/jpeg;base64,")
}

